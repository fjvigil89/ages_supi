                  sequence="7"/>


        <!--        Study-->
        <record model="ir.ui.view" id="view_study_form">
            <field name="name">study.form</field>
            <field name="model">study</field>
            <field name="arch" type="xml">
                <form string="Study">
                    <sheet>
                        <div class="oe_title">
                            <label for="name" class="oe_edit_only"/>
                            <h1>
                                <field name="name" required="1"/>
                            </h1>
                        </div>
                        <group>
                            <group>
                                <field name="variable_id" required="1"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="view_study_tree">
            <field name="name">study.tree</field>
            <field name="model">study</field>
            <field name="arch" type="xml">
                <tree string="Studies">
                    <field name="name"/>
                    <field name="variable_id"/>
                </tree>
            </field>
        </record>

        <record id="view_study_search" model="ir.ui.view">
            <field name="name">study.search</field>
            <field name="model">study</field>
            <field name="arch" type="xml">
                <search string="Studies">
                    <field name="name"/>
                    <field name="variable_id"/>
                </search>
            </field>
        </record>

        <record id="action_study" model="ir.actions.act_window">
