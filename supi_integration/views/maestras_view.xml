                    <field name="product_id"/>
                    <field name="planning_id"/>
                    <field name="auditor_id"/>
                </search>
            </field>
        </record>

        <record id="action_planning_studies" model="ir.actions.act_window">
            <field name="name">Planning studies</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">planning.studies</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_planning_studies_search"/>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Create a new plannings studies in your system
