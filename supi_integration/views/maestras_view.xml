        <record id="view_planning_search" model="ir.ui.view">
            <field name="name">planning.search</field>
            <field name="model">planning</field>
            <field name="arch" type="xml">
                <search string="Planning">
                    <field name="name"/>
                    <field name="place_id"/>
                    <field name="state_id"/>
                </search>
            </field>
        </record>

        <record id="action_planning" model="ir.actions.act_window">
            <field name="name">Planning</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">planning</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_planning_search"/>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Create a new plannings in your system
                </p>
            </field>
        </record>

        <menuitem id="menu_planning_supi"
                  name="Planning"
                  action="action_planning"
                  parent="supi_integration.secondary_data_supi"
                  sequence="1"/>


        <!--        PLANNING STUDIES-->
        <record model="ir.ui.view" id="view_planning_studies_form">
            <field name="name">planning.studies.form</field>
            <field name="model">planning.studies</field>
            <field name="arch" type="xml">
                <form string="Planning Studies">
                    <sheet>

                        <group>
