        <record id="view_parameters_search" model="ir.ui.view">
            <field name="name">parameters.search</field>
            <field name="model">parameters</field>
            <field name="arch" type="xml">
                <search string="Parameters">
                    <field name="name"/>
                </search>
            </field>
        </record>

        <record id="action_parameters" model="ir.actions.act_window">
            <field name="name">Parameters</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">parameters</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_parameters_search"/>
